{
  "name": "moose",
  "version": "0.6.0",
  "displayName": "MOOSE for VSCode",
  "title": "MOOSE for VSCode",
  "description": "language support for MOOSE (Multiphysics Object Oriented Simulation Environment).",
  "keywords": [
    "moose",
    "multiphysics"
  ],
  "authors": [
    "Chris Sewell"
  ],
  "publisher": "chrisjsewell",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/chrisjsewell/vscode-moose/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisjsewell/vscode-moose.git"
  },
  "homepage": "https://github.com/chrisjsewell/vscode-moose/blob/master/README.md",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Language Packs"
  ],
  "activationEvents": [
    "onLanguage:moose"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Moose configuration",
      "properties": {
        "moose.exclude.workspaces": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
          ],
          "description": "Specifies the workspace regexes to ignore when searching for MOOSE objects"
        },
        "moose.exclude.relpaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/test/**/*",
            "**/examples/**/*",
            "**/tutorials/**/*",
            "**/.git/**/*",
            "**/node_modules/**/*",
            "**/libmesh/**/*",
            "**/main.C"
          ],
          "description": "Specifies the path regexes (relative to workspaces) to ignore when searching for MOOSE objects"
        },
        "moose.include.modules": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "phase_field",
            "tensor_mechanics"
          ],
          "description": "Specifies the MOOSE module folders to search in for MOOSE Objects (**/modules/<MODULE>/src/<TYPE>/*.C)"
        },
        "moose.include.types": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "auxkernels",
            "kernels",
            "materials",
            "postprocessors",
            "vectorpostprocessors",
            "bcs",
            "ics",
            "mesh"
          ],
          "description": "Specifies the src subfolders to search in for MOOSE Objects (**/framework/src/<TYPE>/*.C)"
        },
        "moose.include.relpaths": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "src/**/*.C"
          ],
          "description": "Specifies additional path regexes (relative to workspaces) to search for MOOSE objects"
        }
      }
    },
    "commands": [
      {
        "command": "moose.ResetMooseObjects",
        "title": "MOOSE: Reset MOOSE Objects Database"
      }
    ],
    "languages": [
      {
        "id": "moose",
        "aliases": [
          "moose",
          "Moose"
        ],
        "extensions": [
          ".i"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "moose",
        "scopeName": "source.moose",
        "path": "./syntaxes/moose.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "moose",
        "path": "./snippets/moose.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/glob-to-regexp": "^0.4.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "path-parse": "^1.0.6",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "glob-to-regexp": "^0.4.0",
    "@types/glob-to-regexp": "^0.4.0",
    "path-parse": "^1.0.6"
  }
}
